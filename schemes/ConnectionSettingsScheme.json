{
  "id": "ConnectionSettings",
  "type": "object",
  "properties": {
    "port": {
      "type": "number",
      "title" : "Port",
      "description" : "Redis connection port"
    },
    "host": {
      "type": "string",
      "title" : "Host",
      "description" : "Redis connection host"
    },
    "name": {
      "type": "string",
      "title" : "Name",
      "description" : "Redis user name"
    },
    "password": {
      "type": "string",
      "title" : "Password",
      "description" : "Redis user password"
    },
    "family": {
      "type": "number",
      "title" : "Family",
      "description" : "Redis ip family id"
    },
    "db": {
      "type": "number",
      "title" : "DB",
      "description" : "Redis database id"
    }
  },
  "required": [
    "port",
    "host",
    "family",
    "db"
  ],
  "default": {
    "port": 6379,
    "host": "127.0.0.1",
    "family": 4,
    "db": 0
